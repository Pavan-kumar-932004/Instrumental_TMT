{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/download",
        "dest": "/app.py"
      },
      {
        "src": "/thankyou",
        "dest": "/app.py"
      },
      {
        "src": "/game/api",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }
  